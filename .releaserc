branches:
  - master
  - name: alpha
    prerelease: true
debug: true
ci: false
dryRun: false
plugins:
- - "@semantic-release/commit-analyzer"
  - preset: angular
    parserOpts:
      noteKeywords:
      - BREAKING CHANGE
      - BREAKING CHANGES
      - BREAKING
- - "@semantic-release/release-notes-generator"
  - preset: angular
    parserOpts:
      noteKeywords:
      - BREAKING CHANGE
      - BREAKING CHANGES
      - BREAKING
    writerOpts:
      commitsSort:
      - subject
      - scope

